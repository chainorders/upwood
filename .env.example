# Compose-wide defaults (copy to .env and adjust)
POSTGRES_USER=concordium_rwa_dev_user
POSTGRES_PASSWORD=concordium_rwa_dev_pswd
POSTGRES_DB=concordium_rwa_dev
POSTGRES_PORT=5432
DB_POOL_MAX_SIZE=10

# Backend API bind
API_SOCKET_ADDRESS=0.0.0.0
API_SOCKET_PORT=3001

# AWS credential resolution (use your host ~/.aws credentials)
AWS_PROFILE=default
AWS_SDK_LOAD_CONFIG=1
# Optionally provide region via env instead of hardcoding
# AWS_REGION=eu-west-2
# AWS_DEFAULT_REGION=eu-west-2

# Logging
RUST_LOG=info

